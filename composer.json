{
  "name": "sindria/druvel",
  "description": "Micro-framework mvc OOP for drupal 7 module development like laravel design pattern",
  "require": {
    "php": "^5.6 || ^7.0",
    "symfony/var-dumper": "^4.3",
    "guzzlehttp/guzzle": "^6.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Sindria Inc.",
      "email": "luca.pitzoi@sindria.org"
    }
  ],
  "autoload": {
    "classmap": [
      "database/migrations",
      "database/seeds"
    ],
    "psr-0": {
      "Druvel_": "src/"
    },
    "files": [
      "src/Druvel/Helpers/helpers.php",
      "src/Druvel/Helpers/druvel.php"
    ]
  }
}
